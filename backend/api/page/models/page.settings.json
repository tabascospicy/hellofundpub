{
  "kind": "collectionType",
  "collectionName": "pages",
  "info": {
    "name": "Pages",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Slug": {
      "type": "uid"
    },
    "Title": {
      "type": "string"
    },
    "Description": {
      "type": "text"
    },
    "Image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "NavigationOverlayColor": {
      "type": "string",
      "regex": "^#(?:[0-9a-fA-F]{3}){1,2}$"
    },
    "WelcomeBackgroundColor": {
      "type": "string",
      "regex": "^#(?:[0-9a-fA-F]{3}){1,2}$"
    },
    "DonationButtonColor": {
      "type": "string",
      "default": "#40916c",
      "regex": "^#(?:[0-9a-fA-F]{3}){1,2}$"
    },
    "ManagedContent": {
      "type": "dynamiczone",
      "components": [
        "custom.title",
        "custom.date-location-description",
        "custom.description",
        "custom.carousel"
      ]
    },
    "VideoURL": {
      "type": "string"
    },
    "BackgroundImage": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "PresentationImage": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "ButtonsGroup": {
      "type": "dynamiczone",
      "components": [
        "groups.buttons"
      ],
      "required": false
    }
  }
}
