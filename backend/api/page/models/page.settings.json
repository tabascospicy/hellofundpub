{
  "kind": "collectionType",
  "collectionName": "pages",
  "info": {
    "name": "Pages",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Slug": {
      "type": "uid"
    },
    "Title": {
      "type": "string",
      "default": "This is the default title, you can change it on the configuration page "
    },
    "Description": {
      "type": "text",
      "default": "Description of your Fundraising, Donation, Store, Custom, RSVP, Event"
    },
    "Image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "NavigationOverlayColor": {
      "type": "string",
      "regex": "^#(?:[0-9a-fA-F]{3}){1,2}$",
      "default": "#2dd3cc"
    },
    "WelcomeBackgroundColor": {
      "type": "string",
      "regex": "^#(?:[0-9a-fA-F]{3}){1,2}$",
      "default": "#FFF"
    },
    "DonationButtonColor": {
      "type": "string",
      "default": "#2dd3cc",
      "regex": "^#(?:[0-9a-fA-F]{3}){1,2}$"
    },
    "ManagedContent": {
      "type": "dynamiczone",
      "components": [
        "custom.title",
        "custom.date-location-description",
        "custom.description"
      ]
    },
    "VideoURL": {
      "type": "string"
    },
    "BackgroundImage": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "PresentationImage": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "EventDay": {
      "type": "date"
    },
    "Location": {
      "type": "text"
    },
    "ButtonsGroup": {
      "type": "dynamiczone",
      "components": [
        "groups.buttons"
      ],
      "required": false
    },
    "ExtraContent": {
      "type": "dynamiczone",
      "components": [
        "custom.carousel",
        "custom.footer",
        "custom.video",
        "custom.location"
      ]
    }
  }
}
